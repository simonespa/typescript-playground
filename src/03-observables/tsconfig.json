{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Node 16",
  "compilerOptions": {
    "sourceMap": true,
    "lib": [
      "DOM",
      "ES2021"
    ],
    "module": "ES2020",
    "moduleResolution": "Node",
    "outDir": "dist",
    "target": "ES2021",
    "strict": true,
    "noImplicitAny": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["src/**/*"],
  "exclude": ["node_modules"]
}
